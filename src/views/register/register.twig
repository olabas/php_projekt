{% extends 'base.twig' %}

{% block title %}Baza Korepetycji - Rejestracja{% endblock %}

{% block content %}
<div class="panel panel-default contain">
    <div class="panel-heading">
        <h3 class="panel-title title">Panel rejestracji</h3>
    </div>
    <div class="panel-body">
    <form action="{{ path('register_register') }}" method="post" class="form center-block parts">
        <div class="form-group">
            {{ form_widget(form) }}
        </div>
        <button class="btn btn-primary button">{{ 'Register'|trans }}</button>
    </form>
        </div>
    </div>
{% endblock %}

